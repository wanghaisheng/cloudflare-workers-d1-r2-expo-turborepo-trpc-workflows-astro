                  placeholderTextColor="#888"
                  onChangeText={(email) => setEmailAddress(email)}
                />
                <TextInput
                  style={styles.input}
                  value={password}
                  placeholder="Password"
                  placeholderTextColor="#888"
                  secureTextEntry={true}
                  onChangeText={(password) => setPassword(password)}
